algo zgMa decisionTree {
    splitter = 'random'
    max_depth = 8
}

data fRFTGD {
    source = '../datasets/titanic__Survived.csv'
    drop = 'Fare' 'Sex'
}

algo URQRSAO knn {
    weights = 'uniform'
}

data eGjgETp {
    source = '../datasets/random-dataset__Class.csv'
}

algo MVdI svm {
    C = 0.6002271059668445
}

trainer {
    data = data.eGjgETp
    model = algo.URQRSAO
    train_test_split = 0.951183882421597
    show_metrics = true
}

