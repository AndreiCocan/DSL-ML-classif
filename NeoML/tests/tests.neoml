data myData {
   source = "C:/..."
   label = "myClassToPredict"
   drop = "unusedFeature1" "unusedFeature2"
   scaler = MinMax
}

algo myFirstModel svm {
   C = 0.0
}

algo mySecondModel knn {
   n_neighbors = 8
   weights = distance
}

algo myThirdModel decisionTree {
  
}

algo myMLPModel mlp {
   hidden_layer_sizes = 100
}

trainer {
