data myData {
   source = "tests/datasets/Iris.csv"
   label = "Species"
   //drop = "unusedFeature1" "unusedFeature2"
   scaler = MinMax
}

algo myFirstModel svm {
   C = 1.0
   kernel = linear
}

algo mySecondModel knn {
   n_neighbors = 8
   weights = distance
}

algo myThirdModel decisionTree {
   criterion = gini
   max_depth = 10
   splitter = random
}

algo myMLPModel mlp {
   hidden_layer_sizes = 8 7 8
}

trainer {
    data = data.myData
    model = algo.myFirstModel
    train_test_split=0.1
    show_metrics = true
}